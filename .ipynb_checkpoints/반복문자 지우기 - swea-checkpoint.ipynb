   "source": [
    "T = int(input())\n",
    "for tc in range(1, T+1):\n",
    "    sen = input()\n",
    "    lst=[]\n",
    "    #문장을 돌며 이전에 담긴 문자와 담으려고 하는 문자가 같다면 제거3\n",
    "    for s in sen:\n",
    "        if lst and s == lst[-1]:\n",
    "            lst.pop()\n",
    "        else:\n",
    "            lst.append(s)\n",
    "    print('#{}'.format(tc), len(lst))"
   ]
