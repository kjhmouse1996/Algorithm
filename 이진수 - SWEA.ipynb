   "source": [
    "T = int(input())\n",
    "for tc in range(1,T+1):\n",
    "    N,S = input().split()\n",
    "    ten=int('0x'+S,16) # 16진수를 10진수로 바꾸기\n",
    "    #print(ten)\n",
    "    b = format(ten,'b') # 10진수를 2진수로 바꾸기\n",
    "    if len(b) < int(N)*4: # 첫자리가 0인경우는 안보이니 계산해서 붙여주기\n",
    "        b = '0'+b\n",
    " \n",
    "    print('#{} {}'.format(tc,b))"
   ]
